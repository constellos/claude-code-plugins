{
  "$schema": "https://developer.microsoft.com/json-schemas/tsdoc/v0/tsdoc.schema.json",
  "extends": ["tsdoc/tsdoc.json"],
  "tagDefinitions": [
    {
      "tagName": "@context",
      "syntaxKind": "block",
      "allowMultiple": true
    },
    {
      "tagName": "@aliases",
      "syntaxKind": "block",
      "allowMultiple": true
    }
  ],
  "supportForTags": {
    "@context": true,
    "@aliases": true
  }
}
