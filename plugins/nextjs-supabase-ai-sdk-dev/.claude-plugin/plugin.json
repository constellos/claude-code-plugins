{
  "name": "nextjs-supabase-ai-sdk-dev",
  "version": "0.1.2",
  "description": "Next.js, Supabase, and AI SDK development utilities and hooks",
  "author": {
    "name": "constellos"
  },
  "repository": "https://github.com/constellos/claude-code-plugins",
  "license": "MIT",
  "keywords": ["nextjs", "supabase", "ai-sdk", "development"],
  "mcpServers": {
    "next-devtools": {
      "command": "npx",
      "args": ["-y", "next-devtools-mcp@latest"]
    },
    "shadcn": {
      "command": "npx",
      "args": ["-y", "mcp-remote@latest", "https://ui.shadcn.com/docs/mcp"]
    },
    "ai-elements": {
      "command": "npx",
      "args": ["-y", "mcp-remote@latest", "https://registry.ai-sdk.dev/api/mcp"]
    },
    "supabase": {
      "command": "npx",
      "args": ["-y", "@supabase/mcp-server-supabase@latest", "--access-token", "${SUPABASE_ACCESS_TOKEN}"]
    },
    "vercel": {
      "command": "npx",
      "args": ["-y", "mcp-remote@latest", "https://mcp.vercel.com"]
    }
  }
}
